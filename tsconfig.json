{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "outDir": "dist",
    "jsx": "react"
  },
  "include": [
    "./typings/index.d.ts",
    "./src/**/*"
  ]
}
